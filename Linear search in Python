n = int(input("Enter a number:"))
a = []
for i in range(n):
    a.append(int(input()))
print(a)
x = int(input("Enter element to be searched:"))
i = 0
p = 0
for i in range(n):
    if a[i] == x:
        pos = i
        print("element found at ",i+1,"th position")
if not pos:
    print("element not found")
