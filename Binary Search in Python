def bsearch(a,x,l,h):
    mid = int((h+l)/2)
    if x == a[mid]:
        return mid;
    elif x > a[mid]:
        l = mid+1
    elif x < a[mid]:
        h = mid-1
    return bsearch(a,x,l,h)
n = int(input("Enter a number:"))
a = []
for i in range(n):
    a.append(int(input()))
x = int(input("Enter element to be searched:"))
p = bsearch(a,x,0,n)
print("Element found in position",p+1)
